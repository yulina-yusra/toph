#include <stdio.h>
int main()
{
   double m,p,n;
   int t,i;
   scanf("%d", &t);
   for(i=1;i<=t;i++)
   {
       scanf("%lf %lf %lf", &m,&p,&n);
       if(p*n/100.0>m)
       {
           printf("Case %d: NO\n", i);
       }
       else
       {
           printf("Case %d: YES\n", i);
       }
   }
   return 0;
}
